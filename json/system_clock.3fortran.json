(32bit)=', count_max32\n   print g, 'COUNT_RATE(32bit)=', count_rate32\n   print g, 'CURRENT COUNT(32bit)=', count32\n\n   print g, 'time some computation'\n   call system_clock(start64)\n\n   ! some code to time\n   sum = 0.0_wp\n   do i = -0, huge(0) - 1\n      sum = sum + sqrt(real(i))\n   end do\n   print g, 'SUM=', sum\n\n   call system_clock(finish64)\n\n   time_read = (finish64 - start64)/real(count_rate64, wp)\n   write (*, '(1x,a,1x,g0,1x,a)') 'time : ', time_read, ' seconds'\n\nend program demo_system_clock\n"]},{"t":"Para","c":[{"t":"Str","c":"Results:"}]},{"t":"CodeBlock","c":[["",["text"],[]]," >  accuracy may vary with argument type!\n >  query all arguments\n >  COUNT_MAX(64bit)= 9223372036854775807\n >  COUNT_RATE(64bit)= 1000000000\n >  CURRENT COUNT(64bit)= 1105422387865806\n >  COUNT_MAX(32bit)= 2147483647\n >  COUNT_RATE(32bit)= 1000\n >  CURRENT COUNT(32bit)= 1105422387\n >  time some computation\n >  SUM= 66344288183024.266\n >  time :  6.1341038460000004  seconds\n"]},{"t":"Header","c":[3,["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"Standard"}]}]]},{"t":"Para","c":[{"t":"Str","c":"Fortran"},{"t":"Space"},{"t":"Str","c":"95"}]},{"t":"Header","c":[3,["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"See"},{"t":"Space"},{"t":"Str","c":"Also"}]}]]},{"t":"Para","c":[{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"date_and_time"}]},{"t":"Str","c":"(3)"}],["#date_and_time",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"cpu_time"}]},{"t":"Str","c":"(3)"}],["#cpu_time",""]]}]},{"t":"Para","c":[{"t":"Emph","c":[{"t":"Str","c":"fortran-lang"},{"t":"Space"},{"t":"Str","c":"intrinsic"},{"t":"Space"},{"t":"Str","c":"descriptions"}]}]}],"pandoc-api-version":[1,17,5,4],"meta":{}}
