210\n   i =int(b'1111111111111111',kind=int16)\n   !          ^^^^\n   j=ibits(i,10,4) ! start at 10th from left and proceed\n                   ! left for a total of 4 characters\n   write(*,'(a,b16.16)')'j=',j\n  ! lets do something less ambiguous\n   i =int(b'0010011000000000',kind=int16)\n   j=ibits(i,9,5)\n   write(*,'(a,b16.16)')'j=',j\nend program demo_ibits\n"]},{"t":"Para","c":[{"t":"Str","c":"Results:"}]},{"t":"CodeBlock","c":[["",["text"],[]]," > 7\n > 7\n > 0000000000000011 3\n > j=0000000000001111\n > j=0000000000010011\n"]},{"t":"Header","c":[3,["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"Standard"}]}]]},{"t":"Para","c":[{"t":"Str","c":"Fortran"},{"t":"Space"},{"t":"Str","c":"95"}]},{"t":"Header","c":[3,["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"See"},{"t":"Space"},{"t":"Str","c":"Also"}]}]]},{"t":"Para","c":[{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"btest"}]},{"t":"Str","c":"(3)"}],["#btest",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"iand"}]},{"t":"Str","c":"(3)"}],["#iand",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"ibclr"}]},{"t":"Str","c":"(3)"}],["#ibclr",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"ibset"}]},{"t":"Str","c":"(3)"}],["#ibset",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"ieor"}]},{"t":"Str","c":"(3)"}],["#ieor",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"ior"}]},{"t":"Str","c":"(3)"}],["#ior",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"mvbits"}]},{"t":"Str","c":"(3)"}],["#mvbits",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"not"}]},{"t":"Str","c":"(3)"}],["#not",""]]}