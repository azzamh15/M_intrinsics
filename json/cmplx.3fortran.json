ise=z8\n   print *, 'LHS=',precise,'RHS=',z8\n\n  ! elemental\n   zthree=cmplx([10,20,30],-1)\n   print *, 'zthree=',zthree\n\n  ! descriptors are an alternative\n   zthree(1:2)%re=[100,200]\n   print *, 'zthree=',zthree\n\nend program demo_aimag\n"]},{"t":"Para","c":[{"t":"Str","c":"Results:"}]},{"t":"CodeBlock","c":[["",["text"],[]],"    Z4= (-3.000000,0.0000000E+00)\n    Z4= (1.234568,1.234568)\n    Z4= 1.234568 1.234568\n    lost precision Z8= (1.23456788063049,-1.23456788063049)\n    kept precision Z8= (1.23456789012346,-1.23456789012346)\n    Z8 defined with constants= (1.11111111111111,2.22222222222222)\n    LHS=   1.11111111111111      RHS= (1.11111111111111,2.22222222222222)\n    zthree= (10.00000,-1.000000) (20.00000,-1.000000) (30.00000,-1.000000)\n    zthree= (100.0000,-1.000000) (200.0000,-1.000000) (30.00000,-1.000000)\n"]},{"t":"Header","c":[3,["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"Standard"}]}]]},{"t":"Para","c":[{"t":"Str","c":"FORTRAN"},{"t":"Space"},{"t":"Str","c":"77,"},{"t":"Space"},{"t":"Str","c":"KIND"},{"t":"Space"},{"t":"Str","c":"added"},{"t":"Space"},{"t":"Str","c":"in"},{"t":"Space"},{"t":"Str","c":"Fortran"},{"t":"Space"},{"t":"Str","c":"90."}]},{"t":"Header","c":[3,["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"See"},{"t":"Space"},{"t":"Str","c":"Also"}]}]]},{"t":"BulletList","c":[[{"t":"Plain","c":[{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"aimag"}]},{"t":"Str","c":"(3)"}],["#aimag",""]]},{"t":"Space"},{"t":"Str","c":"-"},{"t":"Space"},{"t":"Str","c":"Imaginary"},{"t":"Space"},{"t":"Str","c":"part"},{"t":"Space"},{"t":"Str","c":"of"},{"t":"Space"},{"t":"Str","c":"complex"},{"t":"Space"},{"t":"Str","c":"number"}]}],[{"t":"Plain","c":[{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"conjg"}]},{"t":"Str","c":"(3)"}],["#conjg",""]]},{"t":"Space"},{"t":"Str","c":"-"},{"t":"Space"},{"t":"Str","c":"Complex"},{"t":"Space"},{"t":"Str","c":"conjugate"},{"t":"Space"},{"t":"Str","c":"function"}]}],[{"t":