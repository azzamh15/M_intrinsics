nt8),byte\n\n   write(*,'(a)') \"comparing at the bit level, not as whole numbers.\"\n   write(*,'(a)') \"so pay particular attention to the negative\"\n   write(*,'(a)') \"values on this two's complement platform ...\"\n   do i=-128,127,32\n      byte=i\n      write(*,'(sp,i0.4,*(1x,1l,1x,b0.8))')i,bgt(byte,64_int8),byte\n   enddo\n\n   ! see the BGE() description for an extended description\n   ! of related information\n\nend program demo_bgt\n"]},{"t":"Para","c":[{"t":"Str","c":"Results:"}]},{"t":"CodeBlock","c":[["",["text"],[]]," > we will compare other values to 64\n > +0064  F 01000000\n > comparing at the bit level, not as whole numbers.\n > so pay particular attention to the negative\n > values on this two's complement platform ...\n > -0128  T 10000000\n > -0096  T 10100000\n > -0064  T 11000000\n > -0032  T 11100000\n > +0000  F 00000000\n > +0032  F 00100000\n > +0064  F 01000000\n > +0096  T 01100000\n"]},{"t":"Header","c":[3,["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"Standard"}]}]]},{"t":"Para","c":[{"t":"Str","c":"Fortran"},{"t":"Space"},{"t":"Str","c":"2008"}]},{"t":"Header","c":[3,["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"See"},{"t":"Space"},{"t":"Str","c":"Also"}]}]]},{"t":"Para","c":[{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"bge"}]},{"t":"Str","c":"(3)"}],["#bge",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"ble"}]},{"t":"Str","c":"(3)"}],["#ble",""]]},{"t":"Str","c":","},{"t":"SoftBreak"},{"t":"Link","c":[["",[],[]],[{"t":"Strong","c":[{"t":"Str","c":"blt"}]},{"t":"Str","c":"(3)"}],["#blt",""]]}]},{"t":"Para","c":[{"t":"Emph","c":[{"t":"Str","c":"fortran-lang"},{"t":"Space"},{"t":"Str","c":"intrinsic"},{"t":"Space"},{"t":"Str","c":"descriptions"},{"t":"Space"},{"t":"Str","c":"(license:"},{"t":"Space"},{"t":"Str","c":"MIT)"},{"t":"Space"},{"t":"Str","c":"@urbanjost"}]}]}],"pandoc-api-version":[1,17,5,4],"meta":{}}
